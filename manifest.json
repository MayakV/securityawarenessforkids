{
    "name": "Security Awareness for Kids",
    "version": "1.0",
    "description": "Highlight potentially dangerous websites",
    "browser_action": {
        "default_icon": "images/get_started128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "autorun.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ],
    "icons": {
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}
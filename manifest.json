{
    "name": "Security Awareness for Kids",
    "version": "1.0",
    "description": "Highlight potentially dangerous websites",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage"
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}